    "@types/cookie-parser": "^1.4.4",
    "@types/cors": "^2.8.14",
    "@types/express-handlebars": "^6.0.0",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.11",
    "@types/nodemailer-express-handlebars": "^4.0.3",
    "@types/winston": "^2.4.4",
    "axios": "^1.5.1",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "4.18.2",
    "express-handlebars": "^7.1.2",
